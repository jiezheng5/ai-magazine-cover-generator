{
  "api": {
    "key": "${GEMINI_API_KEY}",
    "model": "gemini-pro",
    "version": "latest",
    "endpoint": "https://api.gemini.ai/v1"
  },
  "settings": {
    "mode": "advanced",
    "context": "project-wide",
    "suggestions": true,
    "history": "enabled",
    "max_tokens": 8000
  },
  "features": {
    "code_generation": true,
    "code_review": true,
    "documentation": true,
    "testing": true,
    "refactoring": true
  },
  "completion": {
    "temperature": 0.7,
    "top_p": 0.95,
    "frequency_penalty": 0.5,
    "presence_penalty": 0.5,
    "max_tokens_per_request": 2000
  },
  "context": {
    "include_patterns": [
      "**/*.{js,ts,py,go,rs,java}",
      "**/docs/**/*.md"
    ],
    "exclude_patterns": [
      "**/node_modules/**",
      "**/dist/**",
      "**/.git/**"
    ],
    "max_files": 50,
    "max_file_size": "1MB"
  },
  "analysis": {
    "static_analysis": true,
    "security_checks": true,
    "performance_review": true,
    "best_practices": true,
    "documentation_quality": true
  },
  "integration": {
    "vscode": true,
    "cursor": true,
    "git": true,
    "ci": {
      "github_actions": true,
      "jenkins": false,
      "gitlab_ci": false
    }
  },
  "output": {
    "format": "detailed",
    "colorize": true,
    "show_context": true,
    "include_metadata": true
  },
  "logging": {
    "level": "info",
    "file": ".gemini/gemini.log",
    "max_size": "10MB",
    "max_files": 5
  },
  "security": {
    "token_encryption": true,
    "secure_storage": true,
    "api_key_rotation": "30d"
  }
}
